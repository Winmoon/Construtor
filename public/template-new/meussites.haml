%html{:lang => "pt-br"}
  %head
    %meta{:charset => "UTF-8"}/
    %title Pool Sites - Construtor de sites
    %link{:href => "css/normalize.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/bootstrap.css", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "/js/bootstrap.js", :type => "text/javascript"}
  %body
    / -----INICIO DA MUDANCA NO MENU-------
    .col-md-12{:style => "background-color: #333333"}
      .col-md-3.col-md-pull-05.col-xs-pull-1
        %a{:href => "index.html", :target => "_self"}
          %img.text-left{:alt => "", :height => "80", :src => "images/poolSitesLogo.png", :style => "", :width => "294"}/
      .container
        .col-md-9.col-md-push-055.top25.reponsiveStyleAdjust
          .col-md-1.col-md-offset-1.col-md-push-05
            %a.white{:href => "index.html", :target => "_self"}
            %h6.text-center Home
          .col-md-2.col-md-push-05
            %a.white{:href => "quemsomos.html", :target => "_self"}
            %h6.text-center Quem somos
          .col-md-1.col-md-push-05
            %a.white{:href => "templates.html", :target => "_self"}
            %h6.text-center Templates
          .col-md-1.col-md-push-1
            %a.white{:href => "planos.html", :target => "_self"}
            %h6.text-center Planos
          .col-md-3.col-md-push-1
            %a.white{:href => "oportunidade.html", :target => "_self"}
            %h6.text-center Oportunidade de negócio
          .col-md-1.col-md-push-1
            %a.white{:href => "contato.html", :target => "_self"}
            %h6.text-center Contato
          .col-md-1.col-md-push-1
            %a.white{:href => "login.html", :target => "_self"}
            %h6.text-center LOGIN
    / -----FINAL DA MUDANCA NO MENU-------
    .container
      .row.top30
        %h3.col-md-12.text-center Seja bem vindo a sua conta.
        .col-md-3.col-md-push-1.top30
          %h3.col-md-11.blueBorderTitle.col-md-pull-3 Gerenciar Conta
        .col-md-3.col-md-offset-05.top40{:style => "margin-left: 5.5%"}
          %a{:href => "#", :target => "_self"}
          .btn-group.dropdown
            %button.btn.btn-danger.btn-lg.noBorderRadius{"data-toggle" => "dropdown", :type => "button"}
              + Criar novo site
      .row.top30
        .col-md-4
          .col-md-11.col-md-offset-05.top10
            .redBorderTitle{:style => "margin-left: 3px"}
              %h4{:style => "margin-left: 20px"}
                %b PROJETOS
            %ul.list-group.col-md-14
              %li.list-group-item
                %a{:href => "#", :style => "margin-left: 12px"}
                  %b Meus sites
              %li.list-group-item
                %a{:href => "#"} Faturas e Pagamentos
              %li.list-group-item
                %a{:href => "#"} Domínios
              %li.list-group-item
                %a{:href => "#"} Plano atual
          .col-md-11.col-md-offset-05.top10
            .redBorderTitle{:style => "margin-left: 3px"}
              %h4{:style => "margin-left: 20px"}
                %b CONFIGURAÇÕES
            %ul.list-group.col-md-14
              %li.list-group-item
                %a{:href => "#"} Minha Conta
              %li.list-group-item
                %a{:href => "#"} Logout
        .col-md-8.col-sm-12
          .col-md-12.boxBordersTitle{:style => "margin-left: -5%; margin-bottom: 25px"}
            %h4 Meus sites
          .col-md-5.clearfix
            .col-md-12.col-sm-6
              .caption.blueBorder{:style => "margin-left: 3px;"}
                / [CHANGELOG] replace no nome da classe da cor da border
                %h4{:style => "margin-left: 10px"} Manicure
              .thumbnail.noBorder
                %img.img-responsive{:alt => "...", :height => "256", :src => "images/fundoCinza.jpg", :width => "256"}/
              %a{:href => "#", :target => "_blank"}/
              %h5.col-md-5.col-md-offset-0.col-sm-4.col-sm-offset-1.col-xs-4.col-xs-offset-1 Visualizar
              %button.col-md-6.col-md-offset-055.col-sm-4.col-xs-4.col-xs-push-05.conversionButton.shadowBox.twelve.lineHeight25.bottom20{:type => "button"}
                EDITAR
          .col-md-5.col-md-offset-1.clearfix
            .col-md-12.col-sm-6
              .caption.blueBorder{:style => "margin-left: 3px"}
                / [CHANGELOG] replace no nome da classe da cor da border
                %h4{:style => "margin-left: 10px"} Advogado
              .thumbnail.noBorder
                %img.img-responsive{:alt => "...", :height => "256", :src => "images/fundoCinza.jpg", :width => "256"}/
              %a{:href => "#", :target => "_blank"}/
              %h5.col-md-5.col-md-offset-0.col-sm-5.col-sm-offset-1.col-xs-4.col-xs-offset-1 Visualizar
              %button.col-md-6.col-md-offset-055.col-sm-4.col-xs-4.col-xs-push-05.conversionButton.shadowBox.twelve.lineHeight25.bottom20{:type => "button"}
